<!--
    Document   : analytical.xml
    Created on : 22. únor 2012, 11:59
    Author     : Martin Řehánek
    Description:
        Example of valid record to be imported into application urnnbn-resolver-v2
        through its http rest api.
        Intelectual Entity in this example is of type OTHER_ENTITY.
        The type will be used for everything that cannot be mapped
        to any other Intelectual Entity type . Particulary maps and music sheets. 
        The type of document can be further described in element documentType.
        This is example of music sheet.
        http://code.google.com/p/urnnbn-resolver-v2/
        http://code.google.com/p/urnnbn-resolver-v2/wiki/API
-->
<r:import xmlns:r="http://resolver.nkp.cz/v3/">
    <r:otherEntity>
        <r:titleInfo>
            <!-- název (povinný údaj) -->
            <r:title>Perličky</r:title>
            <!-- další názvové údaje -->
            <r:subTitle>úryvky ze známých melodií pro malé a větší klavíristy</r:subTitle>
        </r:titleInfo>
        <!-- čČNB -->
        <r:ccnb>cnb002266563</r:ccnb>
        <!-- ISBN (konrétně zde u hudebniny nemá význam, obecně ale u otherEntity být může)
        <r:isbn/>
        -->
        <!-- jiný globální identifikátor intelektuální entity -->
        <r:otherId>ISMN:979-0-66056-072-3</r:otherId>
        <!-- druh dokumentu -->
        <r:documentType>hudebnina</r:documentType>
        <!-- zda je dokument e-born, defaultně false -->
        <r:digitalBorn>false</r:digitalBorn>
        <!-- další původce (např. ilustrátor nebo překladatel) -->
        <r:otherOriginator>Borová, Milada</r:otherOriginator>
        <!-- informace o publikování -->
    </r:otherEntity>
    <r:digitalDocument>
        <!-- identifikátor archivátora -->
        <r:archiverId>3</r:archiverId>
        <!-- seznam identifikátorů v rámci registrátora -->
        <r:registrarScopeIdentifiers>
            <r:id type="aleph_id">mzk03:000056568</r:id>
            <r:id type="signatura">Moll-0005.102</r:id>
            <r:id type="signatura_puvodni">Moll-0090.900,AG.T.VI,2</r:id>
            <r:id type="mapy_mzk_cz_id">mzk03_000_056_568_2619321907</r:id>
        </r:registrarScopeIdentifiers>
        <!-- čím/kým byla digitalizace financována 
             pro novou digitalizaci v rámci NDK zde bude vždy hodnota IOP-NDK
        -->
        <r:financed>norské fondy</r:financed>
        <!-- číslo zakázky, u nové digitalizace se nebude uvádět -->
        <r:contractNumber>123</r:contractNumber>
        <!-- technická metadata, která se vážou k ARCHIVNÍ verzi
             digitálního dokumentu -->
        <r:technicalMetadata>
            <!-- typ a verze formátu -->
            <r:format version="1.0">jpeg</r:format>
            <!-- rozsah (v počtu souborů) -->
            <r:extent>5 x jpeg2000;5 x mods-alto;1 x mods</r:extent>
            <!-- rozlišení nastavené na skeneru v dpi-->
            <r:resolution>
                <!-- horizontální -->
                <r:horizontal>1280</r:horizontal>
                <!-- vertikální -->
                <r:vertical>1024</r:vertical>
            </r:resolution>
            <!-- kompresní algoritmus a komprimační poměr -->
            <r:compression ratio="0.3">LZW</r:compression>
            <r:color>
                <!-- barevný model -->
                <r:model>RGB</r:model>
                <!-- bitová hloubka -->
                <r:depth>24</r:depth>
            </r:color>
            <!-- ICC profil -->
            <r:iccProfile>some ICC profile</r:iccProfile>
            <!-- realná velikost obrázků (něktěrého obrázku) v pixelech -->
            <r:pictureSize>
                <r:width>600</r:width>
                <r:height>1000</r:height>
            </r:pictureSize>
        </r:technicalMetadata>
    </r:digitalDocument>
</r:import>