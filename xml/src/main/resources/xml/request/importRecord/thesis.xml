<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : analytical.xml
    Created on : 22. únor 2012, 11:59
    Author     : Martin Řehánek
    Description:
        Example of valid record to be imported into application urnnbn-resolver-v2
        through its http rest api.
        Intelectual Entity in this example is of type THESIS.
        The type will be used for thesis i.e. token produced by candidate 
        of academic degree during their study.
        http://code.google.com/p/urnnbn-resolver-v2/
        http://code.google.com/p/urnnbn-resolver-v2/wiki/API
-->
<r:import xmlns:r="http://resolver.nkp.cz/v2/">
    <!-- školní práce -->
    <r:thesis>
        <r:titleInfo>
            <!-- název (povinný údaj) -->
            <r:title>Basic Model Checking Problems for Stochastic Games</r:title>
            <!-- další názvové údaje -->
            <r:subTitle>Tato práce se zabývá stochastickými tahovými hrami na nekonečných grafech s lineárními výherními podmínkami.</r:subTitle>
        </r:titleInfo>
        <!-- jiný globální identifikátor školní práce -->
        <r:otherId>TODO</r:otherId>
        <!-- druh dokumentu -->
        <r:documentType>Ph.D. Thesis</r:documentType>
        <!-- zda je dokument e-born, defaultně false -->
        <r:digitalBorn>true</r:digitalBorn>
        <!-- hlavní původce (zde povolen pouze autor) -->
        <r:primaryOriginator type="AUTHOR">Václav Brožek</r:primaryOriginator>
        <!-- další původce (např. ilustrátor nebo překladatel) -->
        <r:otherOriginator>Jan Ilustrator</r:otherOriginator>
        <!-- informace o publikování -->
        <r:publication>
            <!-- rok publikace -->
            <r:year>2009</r:year>
        </r:publication>
        <!-- informace o zdrojovém dokumentu. v tomto případě se jedná o výtisk periodika -->
        <r:degreeAwardingInstitution>Fakulta informatiky; Masarykova univerzita</r:degreeAwardingInstitution>
    </r:thesis>
    <r:digitalDocument>
        <!-- identifikátor archivátora -->
        <r:archiverId>3</r:archiverId>
        <!-- seznam identifikátorů v rámci registrátora -->
        <r:registrarScopeIdentifiers>
            <r:id type="OAI_harvester">1239</r:id>
            <r:id type="K4_pid">uuid:38bfc69d-1d10-4822-b7a4-c7531bb4aedl</r:id>
        </r:registrarScopeIdentifiers>
        <!-- čím/kým byla digitalizace financována 
             pro novou digitalizaci v rámci NDK zde bude vždy hodnota IOP-NDK
        -->
        <r:financed>norské fondy</r:financed>
        <!-- číslo zakázky, u nové digitalizace se nebude uvádět -->
        <r:contractNumber>123</r:contractNumber>
        <!-- technická metadata, která se vážou k ARCHIVNÍ verzi
             digitálního dokumentu -->
        <r:technicalMetadata>
            <!-- typ a verze formátu -->
            <r:format version="1.0">jpeg</r:format>
            <!-- rozsah (v počtu souborů) -->
            <r:extent>3 x jpeg2000;3 x mods-alto;1 x mods</r:extent>
            <!-- rozlišení nastavené na skeneru v dpi-->
            <r:resolution>
                <!-- horizontální -->
                <r:horizontal>1280</r:horizontal>
                <!-- vertikální -->
                <r:vertical>1024</r:vertical>
            </r:resolution>
            <!-- kompresní algoritmus a komprimační poměr -->
            <r:compression ratio="0.3">LZW</r:compression>
            <r:color>
                <!-- barevný model -->
                <r:model>RGB</r:model>
                <!-- bitová hloubka -->
                <r:depth>24</r:depth>
            </r:color>
            <!-- ICC profil -->
            <r:iccProfile>some ICC profile</r:iccProfile>
            <!-- realná velikost obrázků (něktěrého obrázku) v pixelech -->
            <r:pictureSize>
                <r:width>600</r:width>
                <r:height>1000</r:height>
            </r:pictureSize>
        </r:technicalMetadata>
    </r:digitalDocument>
</r:import>
